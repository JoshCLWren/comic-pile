{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dice:playground": "vite --host 127.0.0.1 --port 4174 --strictPort",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "npm run build && sh -c 'if [ -n \"$PLAYWRIGHT_WORKERS\" ]; then playwright test --workers=\"$PLAYWRIGHT_WORKERS\"; else playwright test; fi'",
    "test:e2e:ui": "npm run build && playwright test --ui --workers=1",
    "test:e2e:debug": "npm run build && playwright test --debug --workers=1",
    "test:e2e:headed": "npm run build && playwright test --headed --workers=1",
    "test:e2e:quick": "playwright test",
    "test:e2e:parallel": "npm run build && playwright test",
    "test:e2e:prod-smoke": "playwright test --config=playwright.prod.config.ts",
    "test:dice-playground": "playwright test --config=playwright.dice.config.ts",
    "test:dice-playground:update": "playwright test --config=playwright.dice.config.ts --update-snapshots"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.12.0",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "msw": "^2.11.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^7.2.4",
    "vitest": "^3.2.4"
  }
}
